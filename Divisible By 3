x = int(input("How many numbers do you need to check? "))

y = 0
z = 0
for x in range(0,x):
    a = int(input("Enter a number: "))
    if a%3==0:
        print(str(a) + " is divisible by 3.")
        y += 1
    else:
        print(str(a) + " is not divisible by 3.")
        z += 1
 
print("You entered " + str(y) + " number(s) that are divisible by 3.")
print("You entered " + str(z) + " number(s) that are not divisible by 3.")
